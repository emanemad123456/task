#include <iostream>
#include<iostream>
#include<math.h>
#include<algorithm>
#include<bits/stdc++.h>
#include<string.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int array[n];
    for(int i=0;i<n;i++){
        cin>>array[i];
    }
    sort(array,array+n);
    cout<<array[n-2];

    return 0;
}
